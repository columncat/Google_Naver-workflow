{
    "manifest_version": 2,
    "name": "Google-Naver 검색 전환",
    "description": "구글과 네이버 간의 검색결과를 전환합니다. 단축키를 설정할 수 있습니다.",
    "version": "1.0.3",
    "homepage_url": "https://blog.naver.com/columncat",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": [
            "engineRedirect.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "검색 전환"
    },
    "permissions": [
        "activeTab"
    ],
    "commands": {
        "background": {
            "suggested_key": {
                "default": "Alt+Q"
            },
            "description": "검색 전환"
        }
    }
}